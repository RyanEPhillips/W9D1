def linear_search(arr, target):
# code here
    for i in range(len(arr)):
        if arr[i] == target:
            return i
    return -1
# Sample Test Cases
array = [10, 23, 45, 70, 11, 15]
target = 70
print(f"Index of {target}: {linear_search(array, target)}") 

target = 11
print(f"Index of {target}: {linear_search(array, target)}") 

target = 99
print(f"Index of {target}: {linear_search(array, target)}") 
